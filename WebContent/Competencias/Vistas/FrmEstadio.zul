<?page title="Estadio" contentType="text/html;charset=UTF-8"?>
<zk>
	<div align="center">
		<window title="Estadio" border="normal" width="370px">
			<panel>
				<panelchildren>
					<grid>
						<columns>
							<column label="" width="" />
						</columns>
						<rows>
							<row align="left" style="border:none">
								<label value="Nombre"></label>
								<textbox width="88%" id="txtNombre"/>
							</row>
							<row align="left" style="border:none">
								<label value="DirecciÃ³n"></label>
								<textbox width="88%" multiline="true" height="50px" id="txtDireccion"/>

							</row>
							<row align="left" style="border:none">
								<label value="Estado"></label>
								<combobox id="cmbEstado" width="80%"
