Select CASE WHEN C.descripcion in (select descripcion from tipo_ingreso where aplica_representante=true) THEN 'PAGO REPRESENTANTE' ELSE C.descripcion END AS tipoing, SUM(A.monto_abonado) as ingreso, CASE WHEN SUM(A.monto_abonado) is null THEN 0 ELSE SUM(A.monto_abonado) END AS resultadoing from ingreso_documento_acreedor A, documento_acreedor B, tipo_ingreso C, ingreso D where A.codigo_documento_acreedor=B.codigo_documento_acreedor and B.codigo_tipo_ingreso=C.codigo_tipo_ingreso and C.descripcion='INGRESO EXTRAORDINARIO' and D.codigo_ingreso=A.codigo_ingreso and D.fecha_pago BETWEEN $P{Inicio} and $P{Fin} GROUP BY tipoing;]]>
