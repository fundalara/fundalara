	<subDataset name="Ingresos">
		<parameter name="Inicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Inicio}]]></defaultValueExpression>
		</parameter>
		<parameter name="Fin" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Fin}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select CASE WHEN C.descripcion in ('MENSUALIDAD','CUOTA POR INSCRIPCION','CUOTA POR REINSCRIPCION','UNIFORME') THEN 'PAGO REPRESENTANTE' ELSE C.descripcion END AS tipoing, SUM(A.monto_abonado) as ingreso, CASE WHEN SUM(A.monto_abonado) is null THEN 0 ELSE SUM(A.monto_abonado) END AS resultadoing from ingreso_documento_acreedor A, documento_acreedor B, tipo_ingreso C, ingreso D where A.codigo_documento_acreedor=B.codigo_documento_acreedor and B.codigo_tipo_ingreso=C.codigo_tipo_ingreso and C.aplica_representante=true and D.codigo_ingreso=A.codigo_ingreso and D.fecha_pago BETWEEN $P{Inicio} and  $P{Fin} GROUP BY tipoing UNION ALL
Select CASE WHEN C.descripcion in ('MENSUALIDAD','CUOTA POR INSCRIPCION','CUOTA POR REINSCRIPCION','UNIFORME') THEN 'PAGO REPRESENTANTE' ELSE C.descripcion END AS tipoing, SUM(A.monto_abonado) as ingreso, CASE WHEN SUM(A.monto_abonado) is null THEN 0 ELSE SUM(A.monto_abonado) END AS resultadoing from ingreso_documento_acreedor A, documento_acreedor B, tipo_ingreso C, ingreso D where A.codigo_documento_acreedor=B.codigo_documento_acreedor and B.codigo_tipo_ingreso=C.codigo_tipo_ingreso and C.descripcion='PUBLICIDAD' and D.codigo_ingreso=A.codigo_ingreso and D.fecha_pago BETWEEN $P{Inicio} and $P{Fin} GROUP BY tipoing UNION ALL
Select CASE WHEN C.descripcion in ('MENSUALIDAD','CUOTA POR INSCRIPCION','CUOTA POR REINSCRIPCION','UNIFORME') THEN 'PAGO REPRESENTANTE' ELSE C.descripcion END AS tipoing, SUM(A.monto_abonado) as ingreso, CASE WHEN SUM(A.monto_abonado) is null THEN 0 ELSE SUM(A.monto_abonado) END AS resultadoing from ingreso_documento_acreedor A, documento_acreedor B, tipo_ingreso C, ingreso D where A.codigo_documento_acreedor=B.codigo_documento_acreedor and B.codigo_tipo_ingreso=C.codigo_tipo_ingreso and C.descripcion='DONACION' and D.codigo_ingreso=A.codigo_ingreso and D.fecha_pago BETWEEN $P{Inicio} and  $P{Fin} GROUP BY tipoing UNION ALL
Select CASE WHEN C.descripcion in ('MENSUALIDAD','CUOTA POR INSCRIPCION','CUOTA POR REINSCRIPCION','UNIFORME') THEN 'PAGO REPRESENTANTE' ELSE C.descripcion END AS tipoing, SUM(A.monto_abonado) as ingreso, CASE WHEN SUM(A.monto_abonado) is null THEN 0 ELSE SUM(A.monto_abonado) END AS resultadoing from ingreso_documento_acreedor A, documento_acreedor B, tipo_ingreso C, ingreso D where A.codigo_documento_acreedor=B.codigo_documento_acreedor and B.codigo_tipo_ingreso=C.codigo_tipo_ingreso and C.descripcion='INGRESO EXTRAORDINARIO' and D.codigo_ingreso=A.codigo_ingreso and D.fecha_pago BETWEEN $P{Inicio} and $P{Fin} GROUP BY tipoing;]]>
		</queryString>
		<field name="tipoing" class="java.lang.String"/>
		<field name="ingreso" class="java.lang.Double"/>
		<field name="resultadoing" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="Egresos">
		<parameter name="Inicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Inicio}]]></defaultValueExpression>
		</parameter>
		<parameter name="Fin" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Fin}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select CASE WHEN G.nombre in ('NOMINA','BONOS Y VIATICOS','LIQUIDACION EMPLEADO') THEN 'PAGO PERSONAL' ELSE G.nombre END AS tipoegreso, SUM(E.monto_abonado) as egreso, CASE WHEN SUM(E.monto_abonado) is null THEN 0 ELSE SUM(E.monto_abonado) END AS resultadoeg from egreso_cuenta_pagar E, cuenta_pagar F, dato_basico G, egreso H where E.codigo_cuenta_pagar=F.codigo_cuenta_pagar and F.codigo_tipo_egreso=G.codigo_dato_basico and G.nombre in ('NOMINA','BONOS Y VIATICOS','LIQUIDACION EMPLEADO') and H.codigo_egreso=E.codigo_egreso and H.fecha_pago BETWEEN '01/11/2011' and '15/02/2012' GROUP BY tipoegreso UNION ALL
Select CASE WHEN G.nombre in ('NOMINA','BONOS Y VIATICOS','LIQUIDACION EMPLEADO') THEN 'PAGO PERSONAL' ELSE G.nombre END AS tipoegreso, SUM(E.monto_abonado) as egreso, CASE WHEN SUM(E.monto_abonado) is null THEN 0 ELSE SUM(E.monto_abonado) END AS resultadoeg from egreso_cuenta_pagar E, cuenta_pagar F, dato_basico G, egreso H where E.codigo_cuenta_pagar=F.codigo_cuenta_pagar and F.codigo_tipo_egreso=G.codigo_dato_basico and G.nombre='SERVICIO' and H.codigo_egreso=E.codigo_egreso and H.fecha_pago BETWEEN '01/11/2011'and '15/02/2012' GROUP BY tipoegreso UNION ALL
Select CASE WHEN G.nombre in ('NOMINA','BONOS Y VIATICOS','LIQUIDACION EMPLEADO') THEN 'PAGO PERSONAL' ELSE G.nombre END AS tipoegreso, SUM(E.monto_abonado) as egreso, CASE WHEN SUM(E.monto_abonado) is null THEN 0 ELSE SUM(E.monto_abonado) END AS resultadoeg from egreso_cuenta_pagar E, cuenta_pagar F, dato_basico G, egreso H where E.codigo_cuenta_pagar=F.codigo_cuenta_pagar and F.codigo_tipo_egreso=G.codigo_dato_basico and G.nombre='ADQUISICION' and H.codigo_egreso=E.codigo_egreso and H.fecha_pago BETWEEN '01/11/2011' and '15/02/2012' GROUP BY tipoegreso;]]>
		</queryString>
		<field name="tipoegreso" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="egreso" class="java.lang.Double"/>
		<field name="resultadoeg" class="java.lang.Double"/>
		<group name="tipoegreso">
			<groupExpression><![CDATA[$F{tipoegreso}]]></groupExpression>
		</group>
	</subDataset>
	<subDataset name="New Dataset 1">
		<parameter name="Inicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Inicio}]]></defaultValueExpression>
		</parameter>
		<parameter name="Fin" class="java.util.Date">
			<defaultValueExpression><![CDATA[$P{Fin}]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[Select SUM(B.monto_abonado) as ingreso, F.egreso
from ingreso A, ingreso_documento_acreedor B,
(select SUM(C.monto_abonado) as egreso from egreso_cuenta_pagar C, egreso D
where C.codigo_egreso=D.codigo_egreso
and D.fecha_pago between '01/11/2011' and '15/02/2012'
)F
where A.codigo_ingreso=B.codigo_ingreso
and A.fecha_pago between '01/11/2011' and '15/02/2012'
group by F.egreso;]]>
		</queryString>
		<field name="ingreso" class="java.lang.Double"/>
		<field name="egreso" class="java.lang.Double"/>
	</subDataset>
