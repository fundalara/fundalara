	<queryString>
		<![CDATA[select
	material.codigo_material, material.descripcion,
	 sum (cantidad_requerida) as total,
	dato_basico.nombre,
	material.cantidad_existencia as hastaLaFecha

from
	material, actividad, material_actividad_planificada, dato_basico

where
	material.codigo_material = material_actividad_planificada.codigo_material
	and material_actividad_planificada.codigo_planificacion_actividad = actividad.codigo_planificacion_actividad
	and material.reutilizable = 'FALSE'
	and dato_basico.codigo_dato_basico = material.codigo_unidad_medida
	and actividad.fecha_inicio between $P{FechaInicio}  and $P{FechaFin}
	and actividad.fecha_culminacion between $P{FechaInicio} and $P{FechaFin}

group by
	 material.codigo_material, material.descripcion, 	dato_basico.nombre, material.cantidad_existencia

order by
	material.descripcion]]>
	</queryString>
