	<queryString>
		<![CDATA[select r.cedula_rif, (pn.primer_nombre ||' '|| pn.primer_apellido) as nombreCompleto,((di.valor*cc.duracion_inning)/(select sum(valor) from desempenno_individual as diss where diss.codigo_line_up=di.codigo_line_up and diss.codigo_indicador_categoria_competencia=18 ))as efectividad,c.nombre as competencia,cat.nombre as categoriaNombre,eq.nombre as equipoNombre from indicador as i inner join indicador_categoria_competencia as icc on icc.codigo_indicador=i.codigo_indicador inner join desempenno_individual as di on icc.codigo_indicador_categoria_competencia=di.codigo_indicador_categoria_competencia inner join line_up as li on li.codigo_line_up=di.codigo_line_up inner join roster_competencia as rc on rc.codigo_roster_competencia=li.codigo_roster_competencia inner join roster as r on r.codigo_roster=rc.codigo_roster inner join categoria_competencia as cc on rc.codigo_competencia=cc.codigo_competencia inner join competencia as c on c.codigo_competencia=cc.codigo_competencia inner join persona_natural as pn on r.cedula_rif=pn.cedula_rif inner join categoria as cat on cat.codigo_categoria=cc.codigo_categoria inner join equipo as eq on eq.codigo_equipo=r.codigo_equipo  where i.codigo_indicador=25 and cc.codigo_categoria=5 and  rc.codigo_competencia=1 group by r.cedula_rif,di.valor,li.codigo_line_up,cc.duracion_inning,i.abreviatura,di.codigo_line_up,c.nombre,pn.primer_nombre,pn.primer_apellido,cat.nombre,eq.nombre]]>
