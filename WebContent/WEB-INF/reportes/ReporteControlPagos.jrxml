	</parameter>
	<queryString>
		<![CDATA[select A.codigo_cuenta_pagar, B.cedula_rif, B.razon_social, G.nombre as descripcion, to_char(A.fecha_vencimiento, 'dd/mm/yyyy')as fecha_vencimiento, A.saldo,
A.monto_total, ( (current_date) - A.fecha_vencimiento) as DiasVencidos, CASE WHEN ( (current_date) - A.fecha_vencimiento) <= 0 THEN 0 ELSE ( (current_date) - A.fecha_vencimiento) END AS vencidos
from  cuenta_pagar A, dato_basico G, persona_juridica B
where A.estado= 'P' and
A.estatus='A' and
A.saldo<> '0'and
A.codigo_tipo_egreso=G.codigo_dato_basico and
A.cedula_rif=B.cedula_rif and
A.fecha_vencimiento BETWEEN $P{fechavendes} and $P{fechavenhas}
order by DiasVencidos desc]]>
	</queryString>
