	<queryString>
		<![CDATA[select  to_char(C.hora_inicio, 'hh12:mi:ss') as hora_inicio, to_char(C.hora_fin, 'hh12:mi:ss') as hora_fin, D.nombre dia_semana, E.nombre as lapso, F.nombre as categoria, G.nombre as equipo,
H.nombre_manager, I.nombre_monitor
from plan_temporada A, horario_plan_temporada B, horario C, dato_basico D, lapso_deportivo E, categoria F, equipo G,
(
select X.codigo_equipo, X.codigo_plan_temporada,
Z.primer_nombre||' '|| Z.primer_apellido as nombre_manager
from personal_equipo X,
personaL_cargo Y,
persona_natural Z
where X.cedula_rif=Y.cedula_rif
and X.cedula_rif=Z.cedula_rif
and Y.codigo_cargo=166
order by codigo_equipo
) H,
(
select X.codigo_equipo, X.codigo_plan_temporada,
Z.primer_nombre||' '|| Z.primer_apellido as nombre_monitor
from personal_equipo X,
personaL_cargo Y,
persona_natural Z
where X.cedula_rif=Y.cedula_rif
and X.cedula_rif=Z.cedula_rif
and Y.codigo_cargo=167
) I
where A.codigo_plan_temporada=B.codigo_plan_temporada
and B.codigo_horario=C.codigo_horario
and C.dia=D.codigo_dato_basico
and A.codigo_lapso_deportivo=E.codigo_lapso_deportivo
and A.codigo_categoria=F.codigo_categoria
and B.codigo_equipo=G.codigo_equipo
and B.codigo_equipo=H.codigo_equipo
and A.codigo_plan_temporada=H.codigo_plan_temporada
and B.codigo_equipo=I.codigo_equipo
and A.codigo_plan_temporada=I.codigo_plan_temporada
and A.estatus='A'
and A.codigo_lapso_deportivo=$P{LapsoD}
and F.codigo_categoria=$P{categoria}
and G.codigo_equipo=$P{equipo}]]>
	</queryString>
