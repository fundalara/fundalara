	<queryString>
		<![CDATA[select A.cedula_rif, A.razon_social ,B.direccion, to_char(B.fecha_ingreso, 'dd/mm/yyyy') as ingreso, C.nombre as persona,
CASE WHEN (B.direccion) is  null THEN 'NO ESPECIFICÓ' ELSE (B.direccion) END AS direccion1,
CASE WHEN (B.telefono_habitacion) is  null THEN ' ' ELSE (B.telefono_habitacion) END AS Telefono
from persona_juridica A, persona B, dato_basico C
where A.cedula_rif=B.cedula_rif and C.nombre='PROVEEDOR DE SERVICIO'and C.codigo_dato_basico=B.codigo_tipo_persona
order by A.cedula_rif]]>
	</queryString>
