	<queryString>
		<![CDATA[select t.estado_tarea as codigoestadoTarea, cf.codigo_comision as codigo, plan.descripcion as actividad, d.descripcion as tipoActividad,
i.descripcion as instalacion, a.fecha_inicio as fecha, a.hora_inicio as hora, pn.primer_nombre||' '||pn.primer_apellido as ResponsableAct,
db.nombre as comision, dt.nombre as tarea, pf.primer_nombre||' '||pf.primer_apellido as ResponsableTarea, eb.nombre estadoTarea
from actividad a, planificacion_actividad plan,dato_basico d, persona_natural pn,
dato_basico db, tarea_actividad t, dato_basico dt,
comision_familiar cf, familiar_jugador f, persona_natural pf,
instalacion_utilizada iu, instalacion i, dato_basico eb
where
a.codigo_planificacion_actividad=plan.codigo_planificacion_actividad
and plan.codigo_tipo_actividad!=500
and plan.codigo_tipo_actividad=d.codigo_dato_basico
and pn.cedula_rif=a.cedula_rif
and a.codigo_actividad=$P{CodigoActividad}
and t.codigo_actividad=a.codigo_actividad
and t.codigo_tarea=dt.codigo_dato_basico
and t.codigo_comision_familiar=cf.codigo_comision_familiar
and cf.codigo_comision=db.codigo_dato_basico
and f.codigo_familiar_jugador=cf.codigo_familiar_jugador
and f.cedula_familiar=pf.cedula_rif
and a.codigo_instalacion_utilizada=iu.codigo_instalacion_utilizada
and iu.codigo_instalacion=i.codigo_instalacion
and t.estado_tarea=eb.codigo_dato_basico
and a.estatus='A'
and plan.estatus='A'
and pn.estatus='A'
and t.estatus='A'
and d.estatus='A'
and db.estatus='A'
and dt.estatus='A'
and cf.estatus='A'
and f.estatus='A'
and pf.estatus='A'
and iu.estatus='A'
and i.estatus='A'
and eb.estatus='A'

order by  db. nombre, dt.nombre;]]>
