	<queryString>
		<![CDATA[select B.cedula_rif, C.primer_nombre ||' '|| C.primer_apellido AS nombre,case when C.celular IS null then ' ' else C.celular end ,G.fecha_ingreso,D.nombre as Cargo
from  personal B, persona_natural C, dato_basico D, persona G,personal_cargo pc
where B.cedula_rif=C.cedula_rif and  G.cedula_rif=B.cedula_rif and G.cedula_rif= C.cedula_rif and pc.cedula_rif=C.cedula_rif and
D.codigo_dato_basico=pc.codigo_cargo and pc.estatus='A' and G.codigo_tipo_persona=(select codigo_dato_basico from dato_basico where nombre='PERSONAL REMUNERADO')ORDER BY nombre ]]>
	</queryString>
