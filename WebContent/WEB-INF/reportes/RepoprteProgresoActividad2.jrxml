	<parameter name="CodigoActividad" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	dato_basico.nombre, count (dato_basico.codigo_dato_basico) as cantidad

from

	dato_basico, tarea_actividad

where
	dato_basico.codigo_dato_basico = tarea_actividad.estado_tarea
	and tarea_actividad.codigo_actividad = $P{CodigoActividad}

group by
	dato_basico.codigo_dato_basico, dato_basico.nombre]]>
	</queryString>
