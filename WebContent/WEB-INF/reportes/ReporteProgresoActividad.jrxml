	<queryString>
		<![CDATA[select
	 distinct planificacion_actividad.codigo_planificacion_actividad as "CodigoActividad",
	 dato_basico.nombre as "Tipo de Actividad",
	 planificacion_actividad.descripcion as "Descripción Actividad",
	 instalacion.descripcion as "Instalación",
	 actividad.fecha_inicio,
	 actividad.hora_inicio,
	 persona_natural.primer_nombre,
	 persona_natural.primer_apellido


from
	planificacion_actividad, dato_basico, instalacion, actividad, persona_natural


where

	(planificacion_actividad.codigo_tipo_actividad = dato_basico.codigo_dato_basico or dato_basico.parent_codigo_dato_basico = 307)
	and planificacion_actividad.codigo_instalacion_utilizada = instalacion.codigo_instalacion
	and dato_basico.codigo_dato_basico = planificacion_actividad.codigo_tipo_actividad
	and actividad.codigo_planificacion_actividad = planificacion_actividad.codigo_planificacion_actividad
	and persona_natural.cedula_rif = actividad.cedula_rif
and planificacion_actividad.codigo_planificacion_actividad = $P{CodigoActividad}
	and planificacion_actividad.estatus = 'A'
	and dato_basico.estatus = 'A'
	and instalacion.estatus = 'A'
	and actividad.estatus = 'A'
	and persona_natural.estatus = 'A'

order by

	planificacion_actividad.codigo_planificacion_actividad;]]>
	</queryString>
