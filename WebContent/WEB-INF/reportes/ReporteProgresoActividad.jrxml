	<subDataset name="Tabla">
		<parameter name="CodigoActividad" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
	dato_basico.nombre as Estado, tabla.nombretarea

from
	dato_basico, tarea_actividad,
	(select 	distinct tarea_actividad.codigo_tarea as codigotarea,
	dato_basico.nombre as "nombretarea"

	from tarea_actividad, dato_basico

	where 	tarea_actividad.codigo_tarea = dato_basico.codigo_dato_basico

	order by tarea_actividad.codigo_tarea)tabla


where
	dato_basico.codigo_dato_basico = tarea_actividad.estado_tarea
	and tarea_actividad.codigo_actividad = $P{CodigoActividad}
	and tabla.codigotarea = tarea_actividad.codigo_tarea
	and tarea_actividad.estatus = 'A'
	and dato_basico.estatus = 'A'

order by
	tabla.codigotarea]]>
		</queryString>
		<field name="estado" class="java.lang.String"/>
		<field name="nombretarea" class="java.lang.String"/>
		<group name="estado">
			<groupExpression><![CDATA[$F{estado}]]></groupExpression>
		</group>
	</subDataset>
