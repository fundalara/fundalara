	<queryString>
		<![CDATA[select A.descripcion, sum(B.monto) as Total
from tipo_ingreso A, documento_acreedor B
where B.estado= 'P' and A.codigo_tipo_ingreso= B.codigo_tipo_ingreso and
B.fecha_emision  between $P{fechavendes}and $P{fechavenhas}
group by A.descripcion
order by Total]]>
	</queryString>
