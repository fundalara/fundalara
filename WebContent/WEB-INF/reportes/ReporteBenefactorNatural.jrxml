	<queryString>
		<![CDATA[SELECT A.cedula_rif, B.primer_nombre, B.primer_apellido, A.telefono_habitacion, A.direccion
FROM persona A, persona_natural B, dato_basico C
WHERE A.cedula_rif=B.cedula_rif AND
A.codigo_tipo_persona=C.codigo_dato_basico AND
A.estatus='A' AND B.estatus='A' AND C.estatus='A'AND
A.codigo_tipo_persona=(SELECT codigo_dato_basico FROM dato_basico WHERE nombre='BENEFACTOR NATURAL')
ORDER BY B.cedula_rif]]>
	</queryString>
