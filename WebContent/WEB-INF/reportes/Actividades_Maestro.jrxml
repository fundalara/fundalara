	<subDataset name="indicador_actividad">
		<parameter name="actividad" class="java.lang.String"/>
		<queryString>
			<![CDATA[select B.nombre as nombre_categoria, A.nombre as nombre_actividad, C.nombre as nombre_fase, E.nombre_indicador
from actividad_entrenamiento A, categoria B, dato_basico C, indicador_actividad_escala D,
(
select  distinct (X.indicador), Y.nombre as nombre_indicador
from indicador_actividad_escala X, dato_basico Y
where X.indicador=Y.codigo_dato_basico
) E
where A.estatus='A'
and A.codigo_categoria=B.codigo_categoria
and A.fase=C.codigo_dato_basico
and A.codigo_actividad_entrenamiento=D.codigo_actividad_entrenamiento
and D.indicador=E.indicador
order by B.nombre, A.nombre]]>
		</queryString>
		<field name="nombre_categoria" class="java.lang.String"/>
		<field name="nombre_actividad" class="java.lang.String"/>
		<field name="nombre_fase" class="java.lang.String"/>
		<field name="nombre_indicador" class="java.lang.String"/>
		<group name="nombre_categoria">
			<groupExpression><![CDATA[$F{nombre_categoria}]]></groupExpression>
		</group>
		<group name="nombre_fase">
			<groupExpression><![CDATA[$F{nombre_fase}]]></groupExpression>
		</group>
		<group name="nombre_actividad">
			<groupExpression><![CDATA[$F{nombre_actividad}]]></groupExpression>
		</group>
		<group name="nombre_indicador">
			<groupExpression><![CDATA[$F{nombre_indicador}]]></groupExpression>
		</group>
	</subDataset>
