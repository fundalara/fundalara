	<parameter name="cedulajug_1" class="java.lang.String"/>
	<parameter name="fotoJugador" class="java.lang.Object"/>
	<queryString>
		<![CDATA[--SELECT PRINCIPAL
SELECT
--DATOS A TRAERME
  PN.cedula_rif AS cedula_jug, --CEDULA JUGADOR
  PN.primer_nombre AS pnomb_jug,--PRIMER NOMBRE
  PN.segundo_nombre AS snomb_jug, --SEGUNDO NOMBRE
  PN.primer_apellido AS papell_jug, --PRIMER APELLIDO
  PN.segundo_apellido AS sapell_jug, --SEGUNDO APELLIDO
  D1.nombre AS genero_jug, -- GENERO
  to_char(PN.fecha_nacimiento, 'dd/mm/yyyy') AS fnac_jug,-- FECHA NACIMIENTO
  to_char(P.fecha_ingreso, 'dd/mm/yyyy') AS fing_jug, --FECHA INGRESO
  D2.nombre AS pais_nac, --PAIS NACIMEINTO
  D3.nombre AS parroquia_nac, --PARROQUIA NACIMIENTO
  D4.nombre AS municipio_nac, --MUNICIPIO NACIMIENTO
  (SELECT nombre FROM dato_basico
     WHERE dato_basico.codigo_dato_basico = D4.pEstado) AS estado_nac, --ESTADO NACIMIENTO
  D5.nombre AS parroquia_resi, --PARROQUIA RESIDENCIA
  D6.nombre AS municipio_resi, --MUNICIPIO RESIDENCIA
  (SELECT nombre FROM dato_basico
     WHERE dato_basico.codigo_dato_basico = D6.pEstadoResi) AS estado_resi, --ESTADO RESIDENCIA
   P.direccion AS direccion_jug, --DIRECCIÓN JUGADOR
   P.telefono_habitacion AS tlfhab_jug, --TELEFONO HABITACIÓN
   PN.celular AS tlfcel_jug, --TELEFONO CELULAR
   P.correo_electronico AS email_jug, --CORREO ELECTRÓNICO
   P.twitter AS twitter_jug --TWITTER JUGADOR

--FROM PRINCIPAL
FROM
  jugador,
  persona_natural PN,
  persona P,
  dato_basico D1,
  (SELECT codigo_dato_basico, nombre FROM dato_basico) D2, --PAIS NACIMIENTO
  (SELECT codigo_dato_basico, nombre, parent_codigo_dato_basico FROM dato_basico) D3,-- PARROQUIA NACIMIENTO
  (SELECT --SELECT PARA BUSCAR EL ESTADO DE NACIMIENTO
    (SELECT parent_codigo_dato_basico FROM dato_basico
      WHERE dato_basico.codigo_dato_basico = DMUN.parent_codigo_dato_basico) AS pEstado,-- ESTADO NACIMIENTO
    (SELECT nombre FROM dato_basico
      WHERE dato_basico.codigo_dato_basico = DMUN.parent_codigo_dato_basico) AS nombre -- NOMBRE DEL ESTADO

    FROM
      jugador,
      (SELECT codigo_dato_basico,nombre, parent_codigo_dato_basico FROM dato_basico) DMUN --PARA TRAERME EL CODIGO DEL MUNICIPIO NAC

    WHERE
      jugador.cedula_rif=$P{cedulajug_1}
      AND DMUN.codigo_dato_basico=jugador.codigo_parroquia_nacimiento) AS D4,--MUNICIPIO NACIMIENTO

  (SELECT codigo_dato_basico, nombre, parent_codigo_dato_basico FROM dato_basico) D5,--PARROQUIA RESIDENCIA
  (SELECT
    (SELECT parent_codigo_dato_basico FROM dato_basico
      WHERE dato_basico.codigo_dato_basico = DMUN.parent_codigo_dato_basico) AS pEstadoResi, --ESTADO RESIDENCIA
    (SELECT nombre FROM dato_basico
      WHERE dato_basico.codigo_dato_basico = DMUN.parent_codigo_dato_basico) AS nombre --NOMBRE ESTADO

    FROM
      persona P,
      (SELECT codigo_dato_basico,nombre, parent_codigo_dato_basico FROM dato_basico) DMUN --PARA TRAERME EL CODIGO DEL MUNICIPIO RES

    WHERE
      P.cedula_rif=$P{cedulajug_1}
      AND DMUN.codigo_dato_basico=P.codigo_parroquia) AS D6--MUNICIPIO RESIDENCIA

-- WHERE PRINCIPAL
WHERE jugador.cedula_rif=$P{cedulajug_1}
AND PN.cedula_rif=jugador.cedula_rif
AND PN.cedula_rif=P.cedula_rif
AND PN.codigo_genero = D1.codigo_dato_basico
AND D2.codigo_dato_basico=jugador.codigo_pais
AND D3.codigo_dato_basico=jugador.codigo_parroquia_nacimiento
AND D5.codigo_dato_basico=P.codigo_parroquia]]>
	</queryString>
