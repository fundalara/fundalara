	<subDataset name="tablamedico">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[--AFECCIONES
select
DB.nombre AS nomb_af,--NOMBRE DE LA AFECCIÓN
MED.nombre||' ' || MED.apellido AS nombre_med,--NOMBRE COMPLETO DEL MÉDICO
to_char(DM.fecha_informe, 'dd/mm/yyyy') AS fecha_inf,--FECHA DEL INFORME
to_char(DM.fecha_reincorporacion, 'dd/mm/yyyy') AS fecha_rein, --FECHA DE REINCORPORACION
DM.observacion AS obs --OBSERVACIÓN
FROM
medico MED,
dato_medico DM,
afeccion_jugador AJ,
dato_basico DB,
jugador J
WHERE
J.cedula_rif=$P{cedulajug_1}
AND DM.cedula_rif=J.cedula_rif
AND AJ.codigo_afeccion=DB.codigo_dato_basico
AND AJ.codigo_dato_medico=DM.codigo_dato_medico
AND DM.numero_colegio=MED.numero_colegio
AND DB.parent_codigo_dato_basico=54]]>
		</queryString>
		<field name="nomb_af" class="java.lang.String"/>
		<field name="nombre_med" class="java.lang.String"/>
		<field name="fecha_inf" class="java.lang.String"/>
		<field name="fecha_rein" class="java.lang.String"/>
		<field name="obs" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablamedicol">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[--LESIONES
select
DB.nombre AS nomb_les,--NOMBRE DE LA LESIÓN
MED.nombre||' ' || MED.apellido AS nombre_med,--NOMBRE COMPLETO DEL MÉDICO
to_char(DM.fecha_informe, 'dd/mm/yyyy') AS fecha_inf,--FECHA DEL INFORME
to_char(DM.fecha_reincorporacion, 'dd/mm/yyyy') AS fecha_rein, --FECHA DE REINCORPORACION
DM.observacion AS obs --OBSERVACIÓN
FROM
medico MED,
dato_medico DM,
afeccion_jugador AJ,
dato_basico DB,
jugador J
WHERE
J.cedula_rif=$P{cedulajug_1}
AND DM.cedula_rif=J.cedula_rif
AND AJ.codigo_afeccion=DB.codigo_dato_basico
AND AJ.codigo_dato_medico=DM.codigo_dato_medico
AND DM.numero_colegio=MED.numero_colegio
AND DB.parent_codigo_dato_basico=55]]>
		</queryString>
		<field name="nomb_les" class="java.lang.String"/>
		<field name="nombre_med" class="java.lang.String"/>
		<field name="fecha_inf" class="java.lang.String"/>
		<field name="fecha_rein" class="java.lang.String"/>
		<field name="obs" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablasocial">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
INT.nombre AS nom_inst, --NOMBRE INSTITUCIÓN
DB.nombre AS actividad, --ACTIVIDAD
to_char(DS.fecha_inicio,'dd/mm/yyyy') AS fecha_inic --FECHA INICIO
FROM
institucion INT,
dato_basico DB,
dato_social DS,
jugador J
WHERE
J.cedula_rif=$P{cedulajug_1}
AND DS.cedula_rif=j.cedula_rif
AND DS.codigo_institucion=INT.codigo_institucion
AND DS.codigo_actividad_social=DB.codigo_dato_basico]]>
		</queryString>
		<field name="nom_inst" class="java.lang.String"/>
		<field name="actividad" class="java.lang.String"/>
		<field name="fecha_inic" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablaacademico">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
INT.nombre AS nom_inst, --NOMBRE INSTITUCIÓN
DB.nombre AS anno_escolar, --AÑO ESCOLAR
curso_jug.nombre AS curso --CURSO
FROM
(SELECT codigo_dato_basico, nombre FROM dato_basico)curso_jug,
institucion INT,
dato_basico DB,
dato_academico DA,
jugador J
WHERE
J.cedula_rif=$P{cedulajug_1}
AND DA.cedula_rif=j.cedula_rif
AND DA.codigo_institucion=INT.codigo_institucion
AND DA.codigo_curso=curso_jug.codigo_dato_basico
AND DA.codigo_anno_escolar=DB.codigo_dato_basico]]>
		</queryString>
		<field name="nom_inst" class="java.lang.String"/>
		<field name="anno_escolar" class="java.lang.String"/>
		<field name="curso" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablaconducta">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT

MS.nombre AS motivo,
DB.nombre AS sancion,
DC.cantidad AS cantidad,
to_char(DC.fecha_inicio,'dd/mm/yyyy') AS fecha_inicio,
to_char(DC.fecha_fin,'dd/mm/yyyy') AS fecha_fin,
to_char(DC.fecha_ocurrencia,'dd/mm/yyyy') AS ocurrencia,
DC.observacion AS obs

FROM

dato_conducta DC,
jugador,
dato_basico DB,
motivo_sancion M,
(select codigo_dato_basico, nombre FROM dato_basico) MS

WHERE
jugador.cedula_rif=$P{cedulajug_1}
AND DC.cedula_rif=jugador.cedula_rif
AND M.codigo_dato_conducta=DC.codigo_dato_conducta
AND M.codigo_motivo=MS.codigo_dato_basico
AND DC.codigo_tipo_suspension=DB.codigo_dato_basico]]>
		</queryString>
		<field name="motivo" class="java.lang.String"/>
		<field name="sancion" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Integer"/>
		<field name="fecha_inicio" class="java.lang.String"/>
		<field name="fecha_fin" class="java.lang.String"/>
		<field name="ocurrencia" class="java.lang.String"/>
		<field name="obs" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tabladeportivo">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
CAT.nombre AS Categoria,
EQ.nombre AS Equipo,
to_char(RST.fecha_ingreso,'dd/mm/yyyy') AS Fechaing
FROM
categoria CAT,
equipo EQ,
roster RST,
jugador J
WHERE
J.cedula_rif=$P{cedulajug_1}
AND RST.cedula_rif=j.cedula_rif
AND RST.codigo_equipo=EQ.codigo_equipo
AND EQ.codigo_categoria=CAT.codigo_categoria]]>
		</queryString>
		<field name="categoria" class="java.lang.String"/>
		<field name="equipo" class="java.lang.String"/>
		<field name="fechaing" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablalogros">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
DB.nombre AS logro,
LD.nombre AS temporada,
COMP.nombre AS competencia,
CAT.nombre AS categoria,
E.nombre AS equipo

FROM
dato_basico DB,
dato_deportivo DD,
logro_por_jugador LJ,
lapso_deportivo LD,
competencia COMP,
roster R,
categoria CAT,
equipo E,
jugador J


WHERE
J.cedula_rif=$P{cedulajug_1}
AND DD.cedula_rif=J.cedula_rif
AND LJ.codigo_logro=DB.codigo_dato_basico
AND DD.codigo_dato_deportivo=LJ.codigo_dato_deportivo
AND DD.codigo_competencia=COMP.codigo_competencia
AND COMP.codigo_lapso_deportivo=LD.codigo_lapso_deportivo
AND R.codigo_equipo=E.codigo_equipo
AND R.cedula_rif=DD.cedula_rif
AND E.codigo_categoria=CAT.codigo_categoria]]>
		</queryString>
		<field name="logro" class="java.lang.String"/>
		<field name="temporada" class="java.lang.String"/>
		<field name="competencia" class="java.lang.String"/>
		<field name="categoria" class="java.lang.String"/>
		<field name="equipo" class="java.lang.String"/>
	</subDataset>
	<subDataset name="tablaofe">
		<parameter name="cedulajug_1" class="java.lang.String"/>
		<queryString>
			<![CDATA[---------------OFENSIVA-------------------
SELECT
DISTINCT IND.abreviatura,--INDICADOR ESTADISTICO
SUM(DI.valor) AS promedio-- VALOR ACUMULADO DEL INDICADOR

FROM indicador IND,
desempenno_individual DI,
roster_competencia RC,
line_up LU,
indicador_categoria_competencia ICC,
roster R,
jugador J

WHERE
J.cedula_rif=$P{cedulajug_1}
AND R.cedula_rif=J.cedula_rif
AND RC.codigo_roster= R.codigo_roster
AND LU.codigo_roster_competencia= RC.codigo_roster_competencia
AND IND.codigo_modalidad=283 --CODIGO DE OFENSIVA
AND ICC.codigo_indicador=IND.codigo_indicador
AND DI.codigo_indicador_categoria_competencia=ICC.codigo_indicador_categoria_competencia
AND DI.codigo_line_up= LU.codigo_line_up
GROUP BY IND.abreviatura]]>
		</queryString>
		<field name="abreviatura" class="java.lang.String"/>
		<field name="promedio" class="java.lang.Float"/>
	</subDataset>
