														width="150px" align="center" />
													<column
														width="120px" align="left" />
													<column
														width="150px" align="center" />
													<column
														align="center" />
												</columns>
												<rows>
													<row id="rowEquipo"
														height="35px" visible="true">
														<label
															value="Equipo" width="100px" height="25" />
