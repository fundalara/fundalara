															model="@{ctrl.listEquipos}" width="110px" id="cmbEquipo"
															disabled="true" value="--SELECCIONE--">
															<comboitem
																self="@{each=eq}" value="@{eq}" label="@{eq.nombre}" />
														</combobox>
														<label
