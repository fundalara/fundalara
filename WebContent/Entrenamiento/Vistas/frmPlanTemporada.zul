										<comboitem
											self="@{each=categoria}" label="@{categoria.nombre}"
											value="@{categoria}" />
									</combobox>
									<label value="Equipo:"
										id="lblEquipoPV" width="30px" visible="false" />
									<combobox
										model="@{ctrl.listEquiposPV}" width="110px" id="cmbEquipoPV"
										disabled="true" value="--SELECCIONE--" visible="false">
										<comboitem self="@{each=eq}"
											value="@{eq}" label="@{eq.nombre}" />
									</combobox>
								</row>
							</rows>
						</grid>
					</row>
					<row>
						<panel title="Asignacion de Horario"
							collapsible="true">
							<panelchildren>
								<hbox>
									<grid>
										<columns>
											<column width="110px" />
											<column width="130px"
												align="center" />
											<column width="130px" />
											<column width="140px"
												align="center" />
										</columns>
										<rows>
											<row height="30px">
												<label value="DÃ­a"
													width="100px" />
												<combobox width="130px"
													value="--SELECCIONE--" model="@{ctrl.listDiasSemana}"
													id="cmbdia" disabled="true">
													<comboitem
														self="@{each=dia}" value="@{dia}" label="@{dia.nombre}" />
												</combobox>
												<label />
												<label />
											</row>
											<row>
												<label
													value="Hora Inicio" />
												<timebox id="tmbInicio"
													disabled="true" />
												<label
													value="Hora Final     " />
