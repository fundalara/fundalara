										model="@{ctrl.listLapsoDeportivos}" width="130px"
										id="cmbTemporada" value="--SELECCIONE--">
										<comboitem
											self="@{each=temporada}" value="@{temporada}"
											label="@{temporada.nombre}" />
									</combobox>
									<label value="Tipo:" width="30px" />
