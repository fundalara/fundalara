								<checkbox checked="false" />
							</treecell>
						</treerow>
					</treeitem>
					<treeitem>
						<treerow>
							<treecell label="Competencia" />
							<treecell>
								<checkbox />
							</treecell>
						</treerow>
					</treeitem>
					<treeitem>
						<treerow>
							<treecell label="Actividad Complementaria" />
							<treecell>
								<checkbox />
							</treecell>
						</treerow>
					</treeitem>
					<treeitem>
						<treerow>
							<treecell label="Mantenimiento" />
							<treecell>
								<checkbox />
							</treecell>
						</treerow>
					</treeitem>
				</treechildren>
			</treeitem>
		</treechildren>
	</tree>
